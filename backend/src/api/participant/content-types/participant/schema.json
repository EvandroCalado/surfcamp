{
  "kind": "collectionType",
  "collectionName": "participants",
  "info": {
    "singularName": "participant",
    "pluralName": "participants",
    "displayName": "Participant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "isGeneralInterest": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "participants"
    }
  }
}
